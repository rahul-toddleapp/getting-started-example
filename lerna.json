{
  "packages": [
    "packages/*"
  ],
  "version": "1.0.13",
  "command": {
    "publish": {
      "conventionalCommits": true,
      "message": "chore(release): publish",
      "allowBranch": [
        "lerna-publish",
        "release-v*",
        "develop"
      ]
    },
    "ignoreChanges": [
      "*.md"
    ]
  }
}